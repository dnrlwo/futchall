{
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 2020,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "env": {
        "browser": true,
        "node": true,
        "es6": true
    },
    "extends": [
        "airbnb"
    ],
    "plugins": [
        "import",
        "react-hooks",
        "jsx-a11y"
    ],
    "rules": {
        "no-underscore-dangle": "off",
        "react/forbid-prop-types": "off",
        "object-curly-newline": "off"
    }
}